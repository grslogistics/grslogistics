{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "serve": "rm -rf .cache && gatsby develop",
    "build": "npm run check-env && npm run lambdas-build && gatsby build",
    "lambdas-serve": "npm run dev-env npm run check-env && netlify-lambda serve src/lambdas",
    "lambdas-build": "netlify-lambda build src/lambdas",
    "dev": "npm run dev-env npm run check-env && nodemon --exec \"npm run serve\"",
    "check-env": "node scripts/check-env.js GATSBY_RECAPTCHA_KEY RECAPTCHA_SITE_KEY RECAPTCHA_SECRET_KEY YANDEX_USER YANDEX_PASSWORD CONTEXT URL GTM_ID",
    "dev-env": "dotenv -e .env.development"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.7",
    "@fortawesome/fontawesome-free-solid": "^5.0.12",
    "@fortawesome/react-fontawesome": "^0.0.19",
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "formik": "^0.11.11",
    "gatsby": "^1.9.273",
    "gatsby-image": "^1.0.54",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-google-tagmanager": "^1.0.19",
    "gatsby-plugin-netlify-cache": "^0.1.0",
    "gatsby-plugin-netlify-cms": "^2.0.1",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-svg": "^1.1.1",
    "gatsby-plugin-resolve-src": "^1.1.3",
    "gatsby-plugin-robots-txt": "^1.1.0",
    "gatsby-plugin-sharp": "^1.6.48",
    "gatsby-plugin-sitemap": "^1.2.25",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-source-filesystem": "^1.5.35",
    "gatsby-transformer-remark": "^1.7.40",
    "gatsby-transformer-sharp": "^1.6.27",
    "gatsby-transformer-yaml": "^1.5.17",
    "minimist": "^1.2.0",
    "netlify-cms": "^1.9.2",
    "nodemailer": "^4.6.7",
    "nodemailer-smtp-transport": "^2.7.4",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react-broadcast": "^0.7.0",
    "react-collapse": "^4.0.3",
    "react-google-map": "^3.1.1",
    "react-google-maps-loader": "^4.2.2",
    "react-google-recaptcha": "^0.12.0",
    "react-helmet": "^5.2.0",
    "react-id-swiper": "^1.6.3",
    "react-ink": "^6.2.0",
    "react-markdown": "^3.3.4",
    "react-motion": "^0.5.2",
    "react-portal": "^4.1.5",
    "react-text-mask": "^5.4.1",
    "react-with-viewport": "^0.2.2",
    "recaptcha2": "^1.3.2",
    "styled-components": "^3.2.6",
    "styled-components-breakpoint": "^2.0.2",
    "styled-components-grid": "^1.0.0-preview.15",
    "typeface-pt-sans": "^0.0.54",
    "yup": "^0.25.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/react-portal": "^3.0.4",
    "babel-eslint": "^8.2.3",
    "dotenv-cli": "^1.4.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
