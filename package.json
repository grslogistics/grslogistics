    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "http-proxy-middleware": "^0.18.0",
    "iconv-lite": "^0.4.23",
    "netlify-lambda": "^0.4.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.12.0",
    "require-yaml": "^0.0.1"
  }
}
